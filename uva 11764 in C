#include<stdio.h>
int main()
{
   int t,n,i,high,low,ini1,ini2,a=1,ara[100];
   scanf("%d",&t);
   while(t--){
    scanf("%d",&n);
    high=low=0;
    scanf("%d",&ara[0]);
    for(i=1;i<n;i++){
    scanf("%d",&ara[i]);
    if(ara[i]>ara[i-1])
       high++;
    if(ara[i]<ara[i-1])
        low++;
                              }
     printf("Case %d: %d %d\n",a++,high,low);
                                                 }
return 0;
}
