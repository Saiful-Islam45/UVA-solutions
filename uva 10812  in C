#include<stdio.h>
#include<math.h>
int main()
{
    int a,b,m,n,t;
    double i,j,k,l,check1,check2;
    scanf("%d",&t);
    while(t--){
    scanf("%d %d",&a,&b);
    k=(double)(a+b)/2;
    l=(double)(a-b)/2;
    m=k;
    n=l;
    check1=k-m;
    check2=l-n;
    if(check1>0.0||check2>0.0||a<b)
        printf("impossible\n");
    else
        printf("%.0lf %.0lf\n",k,l);

}
return 0;
}
